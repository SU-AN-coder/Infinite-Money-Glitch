# Wallet source: env | generate
WALLET_KEY_SOURCE=env

# Sui network: testnet | devnet | mainnet
SUI_NETWORK=testnet

# Base64 secret key (for keySource=env)
# Example: export from your Sui wallet tooling, keep it secret
SUI_PRIVATE_KEY=

# Optional OpenClaw RPC config
OPENCLAW_BASE_URL=http://127.0.0.1:18789
OPENCLAW_TOKEN=

# Optional contract ids (reserved for module 02)
BOUNTY_PACKAGE_ID=0x
BOUNTY_BOARD_ID=0x

# Run one earner cycle after wallet initialization
RUN_EARNER=false

# Run one spender cycle after earner stage
RUN_SPENDER=false

# Seal package ID for allowlist policy creation
SEAL_PACKAGE_ID=0x

# Seal key servers: objectId[:weight],objectId[:weight],...
# Example: 0xabc:1,0xdef:1,0x123:1
SEAL_KEY_SERVERS=

# Walrus storage epochs
WALRUS_EPOCHS=3

# Run ledger summary output after earn/spend
RUN_LEDGER=false

# Generate checksum-based audit package
RUN_LEDGER_AUDIT=false

# Run full Agent 6-phase cycle (module 05)
RUN_AGENT=false

# Next cycle estimate for reporting (minutes)
AGENT_CYCLE_MINUTES=5

# Run module 06 scripted demo flow (single cycle with act outputs)
RUN_DEMO=false

# ═══════════════════════════════════════════════════════════════════════════════
# EVIDENCE & PROOF
# ═══════════════════════════════════════════════════════════════════════════════

# Output directory for evidence files
EVIDENCE_OUTPUT_DIR=./evidence

# Enable evidence collection during agent cycles
COLLECT_EVIDENCE=true

# ═══════════════════════════════════════════════════════════════════════════════
# CONTRACT DEPLOYMENT
# ═══════════════════════════════════════════════════════════════════════════════

# Sui CLI binary path (optional, defaults to 'sui' in PATH)
SUI_CLI_PATH=sui

# Gas budget for contract deployment (MIST)
DEPLOY_GAS_BUDGET=500000000
